{
  "name": "QuickCal",
  "version": "1.1.0",
  "description": "Quick Clean Cal",
  "main": "server.js",
  "author": "Bryan Yunis <bryan@bryanyunis.com>",
  "repository": { 
    "type": "git",
    "url": "https://github.com/bry-an/QuickCal"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "nodemon ./server.js --ignore src/ -e js,html,css",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "webpack-config-output": "rimraf tmp && mkdir tmp && cross-env NODE_ENV=production node ./scripts/webpack-config-output.js && cross-env NODE_ENV=development node ./scripts/webpack-config-output.js",
    "heroku-postbuild": "npm run build", 
    "test": "jest"
  },
  "engines": {
    "node": ">=5.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest", 
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }, 
    "collectCoverage": false, 
    "collectCoverageFrom": [
       "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.0-beta.26",
    "body-parser": "~1.15.2",
    "cross-env": "~3.1.3",
    "dotenv": "~2.0.0",
    "express": "~4.14.0",
    "jest": "^23.6.0",
    "moment": "~2.17.1",
    "moment-timezone": "~0.5.11",
    "mongoose": "^5.3.7",
    "nodemon": "~1.11.0",
    "reload": "~1.1.0",
    "request": "~2.79.0",
    "serialize-javascript": "~1.3.0",
    "vue-jest": "^3.0.1",
    "vue-server-renderer": "~2.1.8"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-core": "~6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "~6.4.1",
    "babel-plugin-es6-promise": "~1.1.1",
    "babel-plugin-transform-es2015-destructuring": "~6.23.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.1",
    "css-loader": "~0.25.0",
    "es6-promise": "^4.2.5",
    "extract-text-webpack-plugin": "~2.1.2",
    "file-loader": "~0.9.0",
    "js-object-pretty-print": "~0.2.0",
    "node-sass": "^4.9.4",
    "open": "~0.0.5",
    "sass-loader": "~4.1.1",
    "style-loader": "~0.13.1",
    "uglify-js": "~3.0.28",
    "vue": "~2.1.0",
    "vue-loader": "~10.0.0",
    "vue-resource": "~1.0.3",
    "vue-style-loader": "~1.0.0",
    "vue-template-compiler": "~2.1.0",
    "vuex": "~2.1.1",
    "webpack": "~2.7.0",
    "webpack-dev-middleware": "~1.9.0",
    "webpack-hot-middleware": "~2.14.0",
    "webpack-merge": "~2.3.1",
    "webpack-module-hot-accept": "~1.0.4"
  }
}
